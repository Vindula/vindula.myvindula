<metal:meusLinks define-macro="meusLinks">
	<tal:def define="view python:context.restrictedTraverse('@@meusLinks');
					 links view/get_meusLinks;">
							
		<div>Meus Links</div>
		<div tal:repeat="link links">
			<div class="link">
				<a href="" tal:attributes="href link/url"><p tal:content="link/title"></p></a>
				<span tal:content="link/descricao"></span>	
			</div>
			
		</div>
		
	</tal:def>
</metal:meusLinks>	